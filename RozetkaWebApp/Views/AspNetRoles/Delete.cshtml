@using RozetkaWebApp.App_GlobalResources;
@model RozetkaWebApp.Models.AspNetRole

@{
    ViewBag.Title = (("Удалить группу безопасности"));
}

@{
    ViewBag.DetailsTab = (("active"));
    Html.RenderPartial((("_RootNavigator")));
}

<p>
    <h4>@Resources.Вы_хотите_удалить_эту_группу</h4>
</p>

<form asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <input type="submit" value="@Resources._Remove" class="btn btn-danger" />
    <button type="button" class="btn btn-primary" onclick="history.go(-1)">@Resources.Отменить</button>
    <hr />
</form>
<div>

    <dl class="row">
        <dt class="col-sm-2">
            @Resources.Заголовок0
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
            @Resources.Индикатор
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NormalizedName)
        </dd>
        @*<dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ConcurrencyStamp)
            </dt>
            <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ConcurrencyStamp)
            </dd>*@
    </dl>


</div>
