@model RozetkaWebApp.Models.Property
    @{
        ViewBag.FiltersTab = "active";
        Html.RenderPartial("_CatologNavigator");
    }
   <p></p>

<p>
   <h4>Удалить этот фильтр?</h4>
</p>
<div>
    <form asp-action="Delete">
        <input type="hidden" asp-for="PropertyId" />
        <input type="submit" value="Удалить" class="btn btn-danger" /> 
        <button type="button" class="btn btn-primary"  onclick= "history.go(-1)">Отменить</button>
        <hr />
    </form>
    <p></p>

    <dl class="row">
        <dt class = "col-sm-2"> Каталог  </dt>      <dd class = "col-sm-10"> @Html.DisplayFor(model => model.Catalog.Label) </dd>
        <dt class = "col-sm-2"> Заголовок  </dt>    <dd class = "col-sm-10"> @Html.DisplayFor(model => model.Title) </dd>
        <dt class = "col-sm-2"> Этикетка </dt>      <dd class = "col-sm-10"> @Html.DisplayFor(model => model.Label) </dd>
        <dt class = "col-sm-2"> Маска ввода </dt>   <dd class = "col-sm-10"> @Html.DisplayFor(model => model.Mask) </dd>
        <dt class = "col-sm-2"> Формат вывода </dt> <dd class = "col-sm-10"> @Html.DisplayFor(model => model.Format) </dd>
        <dt class = "col-sm-2"> Это число? </dt>    <dd class = "col-sm-10"> @Html.DisplayFor(model => model.IsNumber) </dd>
        <dt class = "col-sm-2"> Описание </dt>      <dd class = "col-sm-10"> @Html.DisplayFor(model => model.Description) </dd>
    </dl>

</div>
