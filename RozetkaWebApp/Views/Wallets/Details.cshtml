@model RozetkaWebApp.Models.Wallett

@{
    ViewBag.WalletsTab = "active";
    Html.RenderPartial("_ProfileNavigator");
}
<p> 
    <h4>Информация о платежной карте?</h4>
</p>


    <div class="form-group">
          <a class="btn btn-success" asp-action="Edit" asp-route-id="@Model.WalletId"> Изменить </a>
          <a class="btn btn-danger" asp-action="Delete" asp-route-id="@Model.WalletId"> Удалить </a>     
           <button type="button" class="btn btn-primary"  onclick= "history.go(-1)">Отменить</button>  
            <hr />
      </div>   
<div>
    <dl class="row">
        <dt class = "col-sm-2">
           Номер карты
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CardNumber)
        </dd>
        <dt class = "col-sm-2">
            Срок работы
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ExpiryDate)
        </dd>
        <dt class = "col-sm-2">
           Владедец карты
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cardholder)
        </dd>
        <dt class = "col-sm-2">
            Тип карты
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CardType)
        </dd>
        <dt class = "col-sm-2">
            CVV-код
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.VerificationCode)
        </dd>      
    </dl>
</div>

