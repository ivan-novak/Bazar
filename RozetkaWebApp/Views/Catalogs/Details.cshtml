@using RozetkaWebApp.App_GlobalResources;
@model RozetkaWebApp.Models.Catalog

@{
    ViewBag.Catalog = Model;
    ViewBag.DelailTab = (("active"));
    ViewBag.returnUrl = "/Portals/details/"+Model.PortalId.ToString();
    Html.RenderPartial((("_CatologNavigator")));


    <div class="row pt-3">
        <div class="form-group col-lg-4">
            <h4> Макет сторінки </h4>
        </div>
        <div class="form-group col-lg-8" style="text-align: right">
            <a class="btn btn-success" asp-action="Edit" asp-route-id="@Model.CatalogId"> @Resources.Изменить </a>
            <button type="button" class="btn btn-primary" onclick="history.go(-1)">@Resources.Отменить</button>
            @{ string Path = "/catalogs/delete/"+Model.CatalogId.ToString();}
        <partial name="_ButtonDeleteDetails" model="@Path" />
        </div>
    </div>
    ViewBag.DisableBack = true;
    ViewBag.DisableCreate = true;


    Html.RenderPartial((("_BlockPаges")), Model);

    <div class="row m-0 p-0">
        <div class="col-sm-2 m-0 p-0">
            @{
                Html.RenderPartial((("_BlockSelectors")));
            }
        </div>
        <div class="col-sm-10 m-0 p-0">
            @{
                Html.RenderPartial((("_MockupCatalog")));
            }
        </div>
    </div>

}




