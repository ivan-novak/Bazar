@using RozetkaWebApp.App_GlobalResources;
@model IEnumerable<RozetkaWebApp.Models.Portal>

@{
    ViewBag.Title= Resources.Index;
}

<h1>@Resources.Порталы</h1>


<table class="table">
     <tbody>
    @foreach (var item in Model) {
        <tr>
            <td>
                <a asp-controller = "Home" asp-action="Catalogs" asp-route-id="@item.PortalId">@Html.DisplayFor(modelItem => item.Title)</a> 
            </td>
        </tr>
    }
    </tbody>
</table>
